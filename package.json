{
  "name": "@ishikawa-masashi/react-split-view",
  "version": "0.0.1",
  "main": "./dist/index.es.js",
  "types": "./types/index.d.ts",
  "scripts": {
    "dev": "vite",
    "clean": "rimraf dist types",
    "watch": "tsc -w | vite build --watch",
    "build": "tsc && vite build",
    "build:lib": "tsc && vite build --config vite.config.lib.ts",
    "build:debug": "node --inspect-brk=5858 ./node_modules/.bin/vite build",
    "lint": "eslint --fix \"src/**/*.{js,ts,tsx}\""
  },
  "dependencies": {},
  "devDependencies": {
    "@emotion/react": "^11.7.1",
    "@emotion/styled": "^11.6.0",
    "@material-ui/core": "^4.12.3",
    "@mui/icons-material": "^5.2.4",
    "@mui/material": "^5.2.4",
    "@types/jest": "^27.0.3",
    "@types/katex": "^0.11.1",
    "@types/node": "^17.0.0",
    "@types/react": "^17.0.37",
    "@types/react-dom": "^17.0.11",
    "@typescript-eslint/eslint-plugin": "^5.7.0",
    "@typescript-eslint/parser": "^5.7.0",
    "eslint": "^8.5.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "jest": "^27.4.5",
    "katex": "^0.15.1",
    "less": "^4.1.2",
    "monaco-editor": "^0.31.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "^6.2.1",
    "rimraf": "^3.0.2",
    "sass": "^1.45.0",
    "ts-jest": "^27.1.2",
    "typescript": "^4.5.4",
    "vite": "^2.7.3",
    "vite-plugin-monaco-editor": "^1.0.10"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "globals": {
      "ts-jest": {
        "tsconfig": "tsconfig.json"
      }
    },
    "testMatch": [
      "**/tests/**/*.test.ts"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/thzt/test-vite-lib"
  },
  "files": [
    "*"
  ]
}
